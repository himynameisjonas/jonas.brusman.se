{
  "name": "jonas.brusman.se",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jonas Brusman",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tinacms build && npm-run-all build:css build:html",
    "build:css": "tailwindcss -i ./src/site/_includes/css/tailwind.css -o ./src/site/css/styles.css --minify",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel --print-label watch:css watch:html",
    "watch:css": "yarn build:css --watch",
    "watch:html": "eleventy --serve",
    "start": "yarn tinacms dev -c 'yarn watch'",
    "start:netlify": "volta run --node=18.18.2 netlify dev --no-open --target-port 8080 -c 'yarn start'"
  },
  "volta": {
    "node": "20.10.0",
    "yarn": "1.22.17"
  },
  "packageManager": "yarn@1.22.17",
  "engines": {
    "node": ">=18.18.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.0.0-alpha.5",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "html-minifier": "4.0.0",
    "jsdom": "19.0.0",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all2": "5.0.0",
    "prettier": "3.1.1",
    "striptags": "3.2.0",
    "tailwindcss": "3.4.0"
  },
  "dependencies": {
    "@tinacms/auth": "^1.0.3",
    "@tinacms/cli": "^1.5.31",
    "express": "^4.18.2",
    "next-tinacms-s3": "^1.3.7",
    "serverless-http": "^3.2.0",
    "tinacms": "^1.5.22"
  }
}
