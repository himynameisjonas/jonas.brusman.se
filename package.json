{
  "name": "jonas.brusman.se",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jonas Brusman",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm-run-all build:css build:html",
    "build:css": "tailwindcss -i ./src/site/_includes/css/tailwind.css -o ./src/site/css/styles.css",
    "build:html": "eleventy",
    "watch": "npm-run-all --parallel watch:css watch:html",
    "watch:css": "yarn build:css --watch",
    "watch:html": "eleventy --serve",
    "start": "yarn watch"
  },
  "volta": {
    "node": "18.18.2",
    "yarn": "1.22.17"
  },
  "engines": {
    "node": ">=18.18.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.1",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.0.0",
    "autoprefixer": "10.4.7",
    "cssnano": "5.1.9",
    "html-minifier": "4.0.0",
    "jsdom": "19.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.14",
    "postcss-cli": "9.1.0",
    "postcss-import": "14.1.0",
    "striptags": "3.2.0",
    "tailwindcss": "3.0.24"
  },
  "dependencies": {
    "@tinacms/cli": "^1.5.31",
    "tinacms": "^1.5.22"
  }
}
