{% comment %} {% picture_element image title "max-h-screen py-3"%} {% endcomment %}
<div class="w-full overflow-hidden relative max-h-screen">
  <img
    class="object-contain object-center max-h-screen py-2"
    src="{{image.path}}"
    width="{{image.originalWidth}}"
    height="{{image.originalHeight}}"
  >
  <img
    src="{{image.dataURIBase64}}"
    class="opacity-0 absolute inset-0 w-full h-full object-cover object-center"
    style="transition:opacity 0.5s;transition-delay:0.25s;height: calc((100vw)/({{image.originalWidth}}/{{image.originalHeight}}));"
    width="{{image.originalWidth}}"
    height="{{image.originalHeight}}"
  >
</div>
