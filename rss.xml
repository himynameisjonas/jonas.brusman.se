---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>jonasforsberg.se</title>
    <link>http://jonasforsberg.se</link>
    <atom:link href="http://jonasforsberg.se/rss.xml" rel="self" type="application/rss+xml" />
    <description>Agile web developer who likes to do high fives and make cool things with Ruby on Rails.</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    <item>
      <title>{{ post.title }}</title>
      <link>http://jonasforsberg.se{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>hello@jonasforsberg.se (Jonas Forsberg)</author>
      <guid>http://jonasforsberg.se{{ post.id }}</guid>
      <description>
        {% if post.flickr %}
          &lt;a href="http://jonasforsberg.se{{ post.url }}"&gt;{{ post.flickr | flickr_medium_image | xml_escape }}&lt;/a&gt;
        {% endif %}
        {{ post.content | xml_escape }}

        &lt;p&gt;
        {% for tag in post.tags %}
          &lt;a href="http://jonasforsberg.se/tag/{{tag}}"&gt;#{{ tag }}&lt;/a&gt;
        {% endfor %}
        &lt;/p&gt;
      </description>
    </item>
    {% endfor %}

  </channel> 
</rss>